class ${8:`(replace-regexp-in-string
 "^_+" ""
 (mapconcat
  #'identity
  (split-string
   ((lambda (a)
      (substring a (let ((case-fold-search nil))
                     (string-match "\\\\(/[A-Z][a-zA-Z0-9]+\\\\)+$" a )
                     ))
      )
    (file-name-sans-extension (buffer-file-name))
    ) "/") "_"))`}